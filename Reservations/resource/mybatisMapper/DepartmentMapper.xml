<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tongxin.mapper.DepartmentMapper">

	<select id="findById" parameterType="long" resultType="com.tongxin.domain.Department">
		select * from t_department where id = #{id}
	</select>

	<select id="getAll" resultType="com.tongxin.domain.Department">
		select * from t_department
	</select>
	
</mapper> 